<UserControl x:Class="Tre.Settings.Pages.Settings_Page"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:uc="clr-namespace:Tre.Settings.UserControls">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Stretch" MaxWidth="800" Margin="50 0">
        <uc:Settings Title="JSON Strict" HasText="True" IsChecked="False" />
        <uc:Settings Title="Display File Path" HasText="True" IsChecked="True" />
        <uc:Settings Title="Repair Duplicate" HasText="True" IsChecked="False" />
        <uc:Settings Title="Notify Duplicate" HasText="True" IsChecked="True" />
        <uc:Settings Title="Use Other Voids" HasText="True" IsChecked="False" />
    </StackPanel>
</UserControl>
